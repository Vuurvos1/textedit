{
	"name": "textedit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"electron": "concurrently --kill-others \"svelte-kit dev\" \"electron src/electron.cjs\"",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "playwright test",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"dev:package": "npm run build && electron-builder --config electron-builder.config.json --dir",
		"electron:start": "electron electron/index.cjs",
		"package": "npm run build && electron-builder --config electron-builder.config.json"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.1",
		"@supabase/supabase-js": "~1.34.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.9.0",
		"typescript": "^4.8.2",
		"vite": "^3.1.0-beta.1"
	},
	"type": "module",
	"dependencies": {
		"@supabase/auth-helpers-svelte": "^0.4.5",
		"@supabase/auth-helpers-sveltekit": "^0.6.10",
		"@sveltejs/adapter-static": "1.0.0-next.41",
		"@sveltejs/adapter-vercel": "1.0.0-next.72",
		"cookie": "0.5.0",
		"dotenv": "^16.0.2",
		"electron": "^20.1.1",
		"svelte-preprocess": "^4.10.7"
	}
}
