.cm-s-textedit .hmd-fold-html kbd {
	font-family: Consolas, 'Liberation Mono', Menlo, Courier, monospace;
}
.cm-s-textedit .CodeMirror-linewidget table,
.cm-s-textedit .hmd-fold-html table {
	border-collapse: collapse;
}
.cm-s-textedit .CodeMirror-linewidget table td,
.cm-s-textedit .CodeMirror-linewidget table th,
.cm-s-textedit .hmd-fold-html table td,
.cm-s-textedit .hmd-fold-html table th {
	padding: 10px;
	border: 1px solid #ccc;
}
.cm-s-textedit .CodeMirror-linewidget kbd,
.cm-s-textedit .hmd-fold-html kbd {
	display: inline-block;
	padding: 3px 5px;
	font-size: 90%;
	line-height: 1em;
	background-color: #fafbfc;
	border: solid 1px #c6cbd1;
	border-bottom-color: #c6cbd1;
	border-bottom-color: #959da5;
	border-radius: 3px;
	box-shadow: inset 0 -1px 0 #959da5;
	vertical-align: baseline;
}
.cm-s-textedit .CodeMirror-linewidget a > img,
.cm-s-textedit .hmd-fold-html a > img {
	border: 0;
}
.cm-s-textedit .cm-header,
.cm-s-textedit .cm-strong {
	font-weight: 600;
}
.cm-s-textedit span.cm-quote {
	color: #666;
}

.cm-s-textedit span.cm-keyword {
	color: var(--code-keyword);
}
.cm-s-textedit span.cm-atom {
	color: #219;
}
.cm-s-textedit span.cm-number {
	color: var(--code-value);
}

.cm-s-textedit span.cm-def {
	color: var(--code-normal);
}
.cm-s-textedit span.cm-variable,
.cm-s-textedit span.cm-variable-2,
.cm-s-textedit span.cm-variable-3 {
	color: var(--code-property);
}
.cm-s-textedit span.cm-operator {
	color: var(--code-operator);
}

.cm-s-textedit span.cm-meta,
.cm-s-textedit span.cm-comment {
	color: var(--code-comment);
}

.cm-s-textedit span.cm-qualifier,
.cm-s-textedit span.cm-string,
.cm-s-textedit span.cm-string-2 {
	color: var(--code-string);
}

.cm-s-textedit span.cm-builtin,
.cm-s-textedit span.cm-property,
.cm-s-textedit span.cm-attribute {
	color: var(--code-function);
}

.cm-s-textedit span.cm-bracket {
	color: var(--code-punctuation);
}

.cm-s-textedit span.cm-tag {
	color: var(--code-tag);
}

.cm-s-textedit span.cm-url {
	color: #4af;
}
.cm-s-textedit span.cm-link {
	color: #4078c0;
	text-decoration: none;
}
.cm-s-textedit span.cm-link:hover {
	text-decoration: underline;
}
.cm-s-textedit span.cm-error {
	color: #f00;
}
.cm-s-textedit span.cm-math,
.cm-s-textedit span.hmd-fold-math-placeholder {
	font-style: italic;
	background: rgba(255, 255, 238, 0.4);
}
.cm-s-textedit span.cm-formatting-math {
	color: #caa;
	font-weight: inherit;
}
.cm-s-textedit .CodeMirror-activeline-background {
	background: #e8f2ff;
}
.cm-s-textedit .CodeMirror-matchingbracket {
	outline: 1px solid grey;
	color: black !important;
}
.cm-s-textedit .CodeMirror-linenumber {
	color: #ccc;
}
.cm-s-textedit .CodeMirror-gutters {
	border-right: 10px solid #fff;
	padding-right: 5px;
}
.cm-s-textedit .CodeMirror-line {
	padding: 0 5px;
}
.cm-s-textedit .CodeMirror-code {
	padding-right: 10px;
}
.cm-s-textedit .CodeMirror-foldmarker {
	display: inline-block;
	vertical-align: middle;
}
.cm-s-textedit span.cm-hmd-toc {
	font-size: 24pt;
	color: #999;
}
