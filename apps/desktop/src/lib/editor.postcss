.cm-s-textedit {
	background: none;
	caret-color: var(--text-base);
	color: var(--text-base);
	font-family: var(--font-text);
}

.CodeMirror-cursor {
	border-left: 1px solid var(--text-base);
}

.CodeMirror-foldmarker {
	color: inherit;
	text-shadow: none;
	font-family: inherit;
	line-height: inherit;
	cursor: pointer;
}

.cm-s-textedit {
	&.CodeMirror .CodeMirror-gutters {
		background: none;
		border: none;
	}

	.cm-scroller {
		padding: 2rem;
	}
}

.cm-s-textedit .hmd-fold-html-stub,
.cm-s-textedit .hmd-fold-code-stub,
.cm-s-textedit.CodeMirror div.HyperMD-hover > .HyperMD-hover-content code,
.cm-s-textedit pre.HyperMD-table-row span.cm-hmd-table-sep,
.cm-s-textedit span.cm-formatting-hashtag,
.cm-s-textedit span.cm-inline-code,
.cm-s-textedit pre.HyperMD-codeblock,
.cm-s-textedit span.cm-formatting-task,
.cm-s-textedit span.cm-hmd-indent-in-quote,
.cm-s-textedit pre.HyperMD-hr,
